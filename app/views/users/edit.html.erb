<%= current_user.errors.full_messages.join(', ') %>
<h1>Edit Profile</h1>
<%= form_for current_user do |f| %>
<div>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</div>
<div>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</div>
<div>
  <%= f.label :email %>
  <%= f.email_field :email %>
</div>
 <%= f.submit %>
<% end %>
